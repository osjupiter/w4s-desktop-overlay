{
  "name": "w4s-desktop-overlay",
  "description": "This is a desktop application for streamers that overlays web pages on your desktop.",
  "version": "0.0.6",
  "license": "MIT",
  "main": "dist/main.js",
  "scripts": {
    "build:release": "ts-node ./build.ts",
    "build": "cross-env NODE_ENV=\"production\" webpack --progress",
    "clean": "del-cli -f dist release",
    "dev": "run-p dev:*",
    "dev:types": "tsc -w -p tsconfig.main.json",
    "dev:webpack": "cross-env NODE_ENV=\"development\" webpack --progress",
    "dev:electron": "wait-on ./dist/index.html && cross-env NODE_ENV=\"development\" electron .",
    "format": "prettier \"**/*.{ts,tsx,json,html,yml,css,scss,md}\" --check --write",
    "start": "cross-env NODE_ENV=\"development\" electron .",
    "test:types": "tsc -p tsconfig.main.json",
    "test:prettier": "prettier \"**/*.{ts,tsx,json,html,yml,css,scss,md}\" --check"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.2.5",
    "@mui/material": "^5.2.8",
    "@types/lodash": "^4.14.178",
    "@types/uuid": "^8.3.4",
    "commander": "^8.3.0",
    "electron-search-devtools": "^1.5.2",
    "electron-store": "^8.0.1",
    "lodash": "^4.17.21",
    "qs": "^6.10.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/plugin-transform-runtime": "^7.16.10",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@emotion/babel-plugin": "^11.7.2",
    "@trivago/prettier-plugin-sort-imports": "^3.1.1",
    "@types/mini-css-extract-plugin": "^2.4.0",
    "@types/node": "^17.0.8",
    "@types/qs": "^6.9.7",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "babel-loader": "^8.2.3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "del-cli": "^4.0.1",
    "electron": "^16.0.7",
    "electron-builder": "^22.14.5",
    "electron-reload": "^2.0.0-alpha.1",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.4.6",
    "npm-run-all": "^4.1.5",
    "sass": "^1.47.0",
    "sass-loader": "^12.4.0",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4",
    "wait-on": "^6.0.0",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  },
  "optionalDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "prettier": "^2.5.1"
  }
}
